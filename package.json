{
  "name": "vite-template-solid-ssr",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "start": "NODE_ENV=production ts-node ./src/server/index.tsx",
    "dev": "NODE_ENV=development vite",
    "build": "concurrently \"npm:build:client\" \"npm:build:server\"",
    "build-watch": "concurrently \"npm:build:client-watch\" \"npm:build:server-watch\"",
    "build:client": "vite build --outDir ./dist/src/client --ssrManifest",
    "build:server": "vite build --outDir ./dist/src/server --ssr src/server/entry.tsx",
    "build:client-watch": "npm run build:client -- --watch",
    "build:server-watch": "npm run build:server -- --watch",
    "serve": "vite preview",
    "dev:ssr": "NODE_ENV=development concurrently \"nodemon --config ./nodemon-server.json\" \"npm:generate-types-watch\"",
    "dev:ssr-entry": "NODE_ENV=development concurrently \"ts-node ./src/server/index.tsx\" \"npm:generate-types-watch\"",
    "dev:ssr-production": "NODE_ENV=production concurrently \"npm:build-watch\" \"npm:generate-types-watch\" \"nodemon --config ./nodemon-server-production.json\"",
    "lint-fix": "eslint --fix .",
    "generate-types": "tsc --pretty --emitDeclarationOnly --skipLibCheck -p ./tsconfig-declaration.json",
    "generate-types-watch": "npm run generate-types -- -w",
    "prepare": "./internals/bundler/prepare.ts --dir-root=\"${PWD}\""
  },
  "license": "MIT",
  "devDependencies": {
    "@swc/core": "^1.2.66",
    "@swc/helpers": "^0.2.12",
    "@types/fs-extra": "^9.0.12",
    "@types/node": "^16.3.3",
    "@types/node-forge": "^0.10.2",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "concurrently": "^6.2.0",
    "cssnano": "^5.0.6",
    "date-fns": "^2.22.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ext": "^0.1.0",
    "eslint-plugin-import": "^2.23.4",
    "execa": "^5.1.1",
    "hostile": "^1.3.3",
    "node-forge": "^0.10.0",
    "nodemon": "^2.0.12",
    "postcss-preset-env": "^6.7.0",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "vite": "^2.4.2",
    "vite-plugin-compression": "^0.3.1",
    "vite-plugin-fonts": "^0.2.2",
    "vite-plugin-solid": "^2.0.1",
    "vite-plugin-windicss": "^1.2.5",
    "vite-tsconfig-paths": "^3.3.13",
    "windicss": "^3.1.5",
    "yargs": "^17.0.1"
  },
  "dependencies": {
    "@rturnq/solid-router": "^0.3.0",
    "@types/connect": "^3.4.35",
    "async-constructor": "^0.4.16",
    "classnames": "^2.3.1",
    "fastify": "^3.19.1",
    "fastify-static": "^4.2.2",
    "fs-extra": "^10.0.0",
    "middie": "^5.3.0",
    "node-html-parser": "^4.1.0",
    "onetime": "^6.0.0",
    "solid-js": "^1.0.3",
    "solid-meta": "^0.27.0",
    "tslib": "^2.3.0"
  }
}
